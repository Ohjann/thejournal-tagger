{
    "manifest_version": 2,
    "name": "TheJournal.ie Commenter Tagger",
    "version": "1.0",

    "permissions": [ "storage" ],

    "content_scripts": [
        {
            "matches": ["*://*.thejournal.ie/*","*://*.thedailyedge.ie/*","*://*.the42.ie/*"],
            "js": ["build/static/js/main.js"]
        }
    ],

    "background": {
        "scripts": ["background-script.js"]
    }
}
